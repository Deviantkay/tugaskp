//PENUNTASAN LOOPING PADA PROGRAM
    
    //A. UNTUK PROGRAM MODIFIKASI CONTOH 2.4
    private static void antiMC(){
        Scanner sc = new Scanner (System.in);
        int s,d;
        System.out.println("APAKAH ANDA INGIN KEMBALI KE PROGRAM 1? YES (1) / NO (2)");
        s = sc.nextInt();
        switch (s) {
            case 1:
                Modcontoh();
                break;
            case 2:
                System.out.println("APAKAH ANDA INGIN MENGAKHIRI PROGRAM? YES (1) / NO (2)");
                d=sc.nextInt();
                switch (d) {
                    case 1:
                        System.out.println("TERIMA KASIH");
                        break;
                    case 2:
                        System.out.println("KAMU AKAN DIKEMBALIKAN KE MENU AWAL");
                        menu();
                        break;
                    default:
                        System.out.println("INPUT TIDAK DIMENGERTI \n");
                        antiMC();
                        break;
                }   
                break;
            default:
                System.out.println("INPUT TIDAK DIMENGERTI \n");
                antiMC();
        } 
    }
    
    //B. UNTUK PROGRAM NAMA VARIABEL
    private static void antiNV(){
        Scanner sc = new Scanner (System.in);
        int s,d;
        System.out.println("APAKAH ANDA INGIN KEMBALI KE PROGRAM 1? YES (1) / NO (2)");
        s = sc.nextInt();
        switch (s) {
            case 1:
                Namvar();
                break;
            case 2:
                System.out.println("APAKAH ANDA INGIN MENGAKHIRI PROGRAM? YES (1) / NO (2)");
                d=sc.nextInt();
                switch (d) {
                    case 1:
                        System.out.println("TERIMA KASIH");
                        break;
                    case 2:
                        System.out.println("KAMU AKAN DIKEMBALIKAN KE MENU AWAL");
                        menu();
                        break;
                    default:
                        System.out.println("INPUT TIDAK DIMENGERTI \n");
                        antiNV();
                        break;
                }   
                break;
            default:
                System.out.println("INPUT TIDAK DIMENGERTI \n");
                antiNV();
        }
    }
    
    //C. UNTUK PROGRAM KALKULASI JAM - MENIT - DETIK
    private static void antiKJMD(){
        Scanner sc = new Scanner (System.in);
